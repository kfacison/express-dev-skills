<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <title>Express Skill List</title>
</head>
<body>
    <a href="/">Home</a>
    <h1>Skill List</h1>
    <ul>
    <% skills.forEach(function(t) { %>
        <li>
        <a href="/skills/<%= skills.indexOf(t) %>"><%= t %></a>
        </li>
    <% }); %>
    </ul>
    <a href="skills/new">Add NEW Skill</a>
</body>
</html>